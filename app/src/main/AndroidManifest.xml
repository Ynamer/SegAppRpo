<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mind.Seg_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Register2"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Register3_Contact"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Re4_qualification"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Home"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Re_LernSelf"
            android:theme="@style/AppTheme.FullScreen"
            android:parentActivityName=".MainActivity"/>

        <activity
            android:name=".member_show"
            android:theme="@style/AppTheme"
            android:parentActivityName=".Home"/>

        <activity
            android:name=".Reception"
            android:theme="@style/AppTheme.FullScreen"/>

        <!-- Parent activity meta-data to support 4.0 and lower -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />


        <provider
            android:name=".SegProvider"
            android:authorities="com.mind.Seg_app"
            android:exported="false" />
    </application>

</manifest>